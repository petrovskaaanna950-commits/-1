#include <iostream>
using namespace std;
void main() {
	setlocale(0, "");
	int a = 42;
	if (a < 0) {
		cout << "Ошибка число меньше 0";
	}
	else if (a > 100) {
		cout << "Ошибка число больше 100";
	}
	else if (a >= 11 and a <= 20) {
		cout << "Вы попали в диапозон 11-20";
	}
	else if (a >= 21 and a <= 30) {
		cout << "Вы попали ви диапозон 21-30";
	}
	else if (a >= 31 and a <= 40) {
		cout << "Вы попали в диапозон 31-40";
	}
	else if (a >= 41 and a <= 50) {
		cout << "Вы попали в диапозон 41-50";
	}
	else if (a >= 51 and a <= 60) {
		cout << "Вы попали в диапозон 51-60";
	}
	else if (a >= 61 and a <= 70) {
		cout << "Вы попали в диапозон 61-70";
	}
	else if (a >= 71 and a <= 80) {
		cout << "Вы попали ви диапозон 71-80";
	}
	else if (a >= 81 and a <= 90) {
		cout << "Вы попали в диапозон 81-90";
	}
	else if (a >= 91 and a <= 100) {
		cout << "Вы попали в диапозон 91-100";
	}
}
