#include <iostream>
using namespace std;
int main() {
	setlocale(0, "");
	int a = 10;
	cout << "a = " << a << " | тип: int, размер: 4 б." << endl;

	int a_1 = 23;
	cout << "a_1 = " << a_1 << " | тип: int, размер: 4 б." << endl;

